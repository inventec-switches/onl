	pci0: pcie@ffe200000 {
		reg = <0xf 0xfe200000 0 0x1000>;
		ranges = <0x02000000 0x0 0xd0000000
                  0x0 0xd0000000
                  0x0 0x08000000

			      0x01000000 0x0 0x00000000
                  0x0 0xf8000000
                  0x0 0x00010000>;
		pcie@0 {
            ranges = <0x02000000 0 0xe0000000
                      0x02000000 0 0xe0000000
				      0 0x08000000

				      0x01000000 0 0x00000000
                      0x01000000 0 0x00000000
				      0 0x00010000>;
		};
	};

	pci1: pcie@ffe201000 {
		reg = <0xf 0xfe201000 0 0x1000>;
		ranges = <0x02000000 0x0 0xd8000000
                  0x0 0xd8000000
                  0x0 0x08000000

			      0x01000000 0x0 0x00000000
                  0x0 0xf8010000
                  0x0 0x00010000>;
		pcie@0 {
            ranges = <0x02000000 0 0xe0000000
                      0x02000000 0 0xe0000000
				      0 0x08000000

				      0x01000000 0 0x00000000
                      0x01000000 0 0x00000000
				      0 0x00010000>;
		};
	};

	pci2: pcie@ffe202000 {
		reg = <0xf 0xfe202000 0 0x1000>;
		ranges = <0x02000000 0x0 0xe0000000
                  0x0 0xe0000000
                  0x0 0x08000000

			      0x01000000 0x0 0x00000000
                  0x0 0xf8020000
                  0x0 0x00010000>;

		pcie@0 {
			ranges = <0x02000000 0 0xe0000000
				      0x02000000 0 0xe0000000
				      0 0x08000000

				      0x01000000 0 0x00000000
                      0x01000000 0 0x00000000
				      0 0x00010000>;
		};
	};

	fsl,dpaa {
		compatible = "fsl,p2041-dpaa", "fsl,dpaa";

		ethernet@0 {
			compatible = "fsl,p2041-dpa-ethernet", "fsl,dpa-ethernet";
			fsl,fman-mac = <&enet3>;
		};

		ethernet@1 {
			compatible = "fsl,p2041-dpa-ethernet", "fsl,dpa-ethernet";
			fsl,fman-mac = <&enet0>;
		};
	};
};

/include/ "fsl/as5710_54x-post.dtsi"

/include/ "fsl/qoriq-dpaa-res1.dtsi"
