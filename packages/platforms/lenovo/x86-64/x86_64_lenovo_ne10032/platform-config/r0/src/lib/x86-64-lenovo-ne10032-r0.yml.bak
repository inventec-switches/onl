######################################################################
#
# platform-config for NE10032
#
######################################################################

x86-64-lenovo-ne10032-r0:

  grub:

    serial: >-
      --port=0x2f8
      --speed=115200
      --word=8
      --parity=no
      --stop=1

    kernel:
      <<: *kernel-3-18

    args: >-
      nopat
      console=ttyS1,115200n8

